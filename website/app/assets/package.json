  "devDependencies": {
    "terser": "^5.39.0",
    "vite": "^6.3.3",
    "vite-plugin-static-copy": "^2.3.1"
  },
  "dependencies": {
    "@codemirror/lang-markdown": "^6.0.1",
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.0",
    "codemirror": "^6.0.1",
    "dayjs": "^1.11.13",
    "jquery": "^3.7.1",
